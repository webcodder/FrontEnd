    <section  class="vbox"  >

<section class="scrollable1 padder " ng-controller="color" select-list-hide-func selectlistshide="selectListShow">
    <div class="w-1000 row" style="width:738px">
        <div class="part3 row" >
        	<div class="top">
        		<p class="title">布姆学院生死局预约</p>

        	</div>
        	<div class="mid animate-steps " >

                <!-- ===============================================
                     选择大区 
                -->
                <!-- ng-show="applyStatue=='apply'" -->
                <div class="animate-step pt0"  ng-show="applyStatue=='apply'">
            		<div class="step step1 ">
            			<p class="title">游戏大区</p>
            			<div class="select">
            				<div class="selected">
            					<span class="selected-txt" ng-bind="areaInit"></span>
            					<span class="fa fa-caret-down custom-btn" ng-click="selectListToggleFunc($event)"></span>
            				</div>
            				<ul class="select-list scrollable " ng-show="selectListShow">
            					<li class="select-li" ng-repeat="row in areaArr" area-id={{row.id}}
                                ng-bind="row.name+'（'+row.code+'）'"
                                ng-click="selectListFunc(row.name+'（'+row.code+'）',row.name)"
                                ></li>
            				</ul>
            			</div>
            		</div>
                    <div class="btns">
                        <div class="btn-public btn1"  ng-click="applyFn()" style="width:226px">
                            <span >确定预约</span>
                        </div>

                    </div>
                </div>



                <!-- ===============================================
                     预约成功 
                -->
                <!-- ng-show="applyStatue=='wait'" -->
                <div class="part4 row" style="border:none;width:326px;" ng-show="applyStatue=='wait'">
                    <div class="result"></div>
                    <p class="result-tip">您已预约成功！</p>
                    <p class="text-left p170">当前队列位置：<b ng-bind="waitDays"></b></p>
                    <p class="text-left p170">游戏大区：<b ng-bind="applyDetail.areaName"></b></p>
                    <p class="text-left p170">辅导老师：<b ng-bind="nowTeacher.name"></b></p>
                    <p class="text-left p170">请联系导师沟通确定具体时间</p>
                    <div class="btns">

                        <a href="tencent://message/?uin={{nowTeacher.qq}}&amp;amp;Site=&amp;amp;Menu=yes" class="btn-public btn1 left"
                        style="width:140px"
                        >
                            <span >联系导师</span>
                        </a>
                        <div class="btn-public btn1 right"  ng-click="ssjApplyCancelFn()" style="width:140px">
                            <span >我要取消预约</span>
                        </div>

                    </div>
            
                </div>

                <!-- ===============================================
                     教学评论
                -->
                <!-- teached -->
                <div class="w-1000 row comment" ng-show="applyStatue=='teached'">

                    <div class="process" >
                        
                        <div class="commment-li row commment-li-2" style="border:none;width:730px; padding: 0 115px;">
                            <div class="comment-top">
                                <div class="ssj-icon"></div>
                                <p class="mb20">您预约的生死局服务已 <b></b>  完成</p>
                                <p class="score">
                                    <span>请对服务导师作出评价   </span>&nbsp;&nbsp;&nbsp;
                                    <span class="stars">


                                        <i class="fa" ng-repeat="star in [0,1,2,3,4]" 
                                        ng-class="score>star?'fa-star':'fa-star-o'"
                                        ng-click="scoreFn($index)"></i>
      
                                    </span>

                                </p>
                                
                            </div>

                            
                            <textarea 

                            class="myComment" 
                            placeholder="你觉得这个老师如何呢？大声的说出你的心声吧！"
                            ng-model="myComment" 
                            maxlength="300" 
                            
                            ></textarea>

                            <div class="relative clearfix " style="padding-top:10px;height: 80px;margin-bottom: 50px;">
                                
                                <p class="tips text-right fr">还可以输入<span><b ng-bind="300-myComment.length">300</b></span>个字符</p>



                                <form class="addImgPart fl" upload-img>

                                    <label for="addImg" class="addImgInput fl"
                                    ng-hide="addImgs.length==3"></label>
                                    <p class="addimgtip fl" ng-hide="addimgtipHide">小提示： 请上传jpeg或jpg格式的图片，图片大小为5KB-1MB
                                    </p> 

                                    <input id="addImg" type ="file" name="file" class="hidden" />
                                    <div class="preview-img fl" ng-repeat="img in addImgs">
                                        <div class="preview-hover" ng-click="deleteImg($index)">
                                            删除
                                        </div>
                                        <div class="preview-in">
                                            
                                            <img ng-src="{{img.smallimg}}">
                                        </div>
                                    </div>

                                    <span class="imgsLen fl" ng-show="addImgs.length>0"><b ng-bind="addImgs.length">0</b>/3</span>
                                                                   
                                </form>

                            </div>



                            <button  class="btn-blue"  style="float:none;" 

                            ng-click="myCommentApply()"
                            
                            >发表评论</button>
                            <p class="text-center text-danger" ng-bind="mycommentTip"></p>
                            
                        </div>
                    </div>
                    

                    <div class="part4 row hidden"  style="padding-bottom: 30px;"  >
                    
                        <div class="result"></div>
                        <p class="result-tip">您已评论成功！</p>
                
                    </div>

                </div>


                <!-- ===============================================
                     预约倒计时
                -->
                <div class="part4 row" style="border:none;width:326px;padding-bottom: 0;" ng-show="applyStatue=='end'">
                    <div class="result" style="background-image: url(css/img/service-watch.png)"></div>
                    <p class="result-tip">您的预约CD倒计时:</p>
                    <div class="text-center"><b class="rspan" ng-bind="waitDays||3"></b><b class="rb">天</b></div>
            
                </div>

            </div>
        	<div class="bottom ">
        		<p class="title">生死局预约说明</p>
        		<p class="li">1. 生死局是晋级赛关键的一局，赢了则可以晋级，输了就失去晋级赛资格；</p>
        		<p class="li">2. 学员预约后可以联系老师确定时间和提交截图确认为生死局，不符合条件的老师有权取消该预约；</p>
        		<p class="li">3. 该服务完成后需要等待3天CD才可以预约下一次；</p>
                <p class="li">4. 布姆学院前5000名学员终身享有生死局导师助阵的服务。</p>
        	</div>
        </div>

        <div class="part4 row" ng-show="StepResult">
			<p class="title">布姆学院1对1教学预约</p>
			<div class="result"></div>
			<p class="result-tip">您已预约成功！</p>
			<p class="result-p">您预约的1对1教学时间为</p>
			<b class="result-time" ng-bind="applyData.teachTime"></b>
			<p class="result-tip-bottom">如有特殊情况，请与老师及时沟通！</p>
	
        </div>
        <div class="part2">
            <div class="w-750" style="width: 738px;">
                
                <div class="comment" style="width:100%">
                    <div class="header1">
                        <p>全部评价<span>（<b ng-bind="teachersComments.totalElement"></b>）</span></p>
                    </div>

                    <div class="main1">
                        <div class="header2">
                            <span class="pageshow">第 <span ng-bind="teachersComments.nowlists"></span>/<span ng-bind="teachersComments.totalElement"></span> 条</span>
                            <div class="paging">
                                <span class="fa fa-angle-left"
                                ng-click="commentChange(pagesArr.prev)"></span>
                                <span class="pagenow">第 <span ng-bind="teachersComments.nowpage"></span> 页</span>
                                <span class="fa fa-angle-right"
                                ng-click="commentChange(pagesArr.next)"></span>
                                <span class="fa fa-caret-down"
                                ng-click="pagesShowTottle1()"
                                ></span>
                                <div class="pages "
                                 ng-show="pagesShow1">
                                    <ul class="scrollable"
                                   >
                                        
                                        <li 
                                        ng-repeat="li in teachersComments.pageList"
                                        ng-bind="li.num+1"
                                        ng-class="li.css"
                                        ng-click="commentChange(li.num)"
                                        
                                        ></li>

                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="comment-lists" >
                            <ul >
                                <li ng-repeat=" list in listsData">

                                    <div class="comment-li row">
                                        <div class="imgpart">
                                            <div class="headerimg" style="background-image: url({{list.oneToOneServiceApply.headimg}})"></div>
                                        </div>
                                        <div class="commentpart">
                                            <p class="name" ng-bind="list.oneToOneServiceApply.name"></p>
                                            <p class="contxt" ng-bind="list.oneToOneServiceApply.studentEvaluate">  
                                            </p>
                                            <div class="infos">
                                                <span class="time" ng-bind="list.oneToOneServiceApply.timeBefore"></span>
                                                <span class="teacher">@
                                                <span ng-bind="list.oneToOneServiceApply.teacherName"></span></span>
                                                <span class="zhichi">
                                                    <span class="fa fa-thumbs-o-up"
                                                    ng-click="applyPraise(student.studentNum,list.oneToOneServiceApply.id,list.$$hashKey)"
                                                    ></span>
                                                <span>（<span ng-bind="list.oneToOneServiceApply.praise"></span>）</span>
                                                </span>
                                            </div>
                                            <div class="imgsShow clearfix" >
                                                <div class="imgShow_out" ng-repeat="img in list.evaluatePhotoList"
                                                img-show
                                                active="0"
                                                >
                                                    <div class="imgShow_in" >
                                                        <img ng-src="{{imgUrl+img.photo+'_40_40.jpg'}}">
                                                    </div>
                                                </div>


         
                                            </div>
                                            <div class="bigimgShow">
                                                <a href="" target="_blank">
                                                    <img src="" class="bigimg">
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                    <hr class="hr" size="1" style="margin-top:20px;">
                                </li>

                            </ul>
                        </div>
                    </div>
                    <div class="footer1">
                        <div class="bpaging">
                            <ul class="pages scrollable"
                            ng-show="pagesShow">
                                <li 
                                ng-repeat="li in teachersComments.pageList"
                                ng-bind="li.num+1"
                                ng-class="li.css"
                                ng-click="commentChange(li.num)"

                                ></li>
                            </ul>
                            <div class="prev"
                            ng-click="commentChange(pagesArr.prev)">
                                <span class="fa fa-angle-left"
                                
                                ></span>上一页
                            </div>
                            <div class="nowpage" ng-click="pagesShowTottle()">
                                <span>第 <b ng-bind="teachersComments.nowpage"></b> 页</span>
                                <span class="fa fa-caret-up"></span>
                            </div>
                            <div class="next"
                            ng-click="commentChange(pagesArr.next)">
                                下一页<span class="fa fa-angle-right"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
