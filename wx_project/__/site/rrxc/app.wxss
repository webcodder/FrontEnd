/* reset */
page {
  font-family: "苹方";
  font-size: 24rpx;
  background: #efefef;
}

button{
  height: 46rpx;
  line-height: 48rpx;
  background: #f5560f;
  color: #fff;
  font-size: 24rpx;
  padding-right:10px;
}

button::after{
  border: none;
}

image{
  width: 100%;
  height: 100%;
}

.clear{
  clear: both;
}

.clearfix::after{
  display:block;
  clear:both;
  visibility:hidden;
  height:0;
  overflow:hidden;
  content:".";
}

.main_cot {
  width: 100%;
  min-height: 100rpx;
  background: #fff;
  margin-bottom: 16rpx;
}

.main_wrap {
    margin: 0 24rpx;
    height: 100%;
    position: relative;
    border-top: solid 1px transparent;
}

.main_botton {
    width: 100%;
    min-height: 100rpx;
    border-top: solid 1px #e2e2e2;
    position: fixed;
    background: #fff;
    bottom: 0;
}

.pudtal_cont{
  margin-bottom: 122rpx;
}
/* /reset */

/* =====================
   template
 ===================== */
/* nav */
.nav_wrap {
  width: 100%;
  background: #fff;
  box-shadow: 0px 1px 1px 1px #dedede;
}

.nav {
  width: 90%;
  margin: 0 auto;
}

.nav ul {
  display: flex;
  justify-content: space-between;
  font-size: 0;
}

.nav li {
  position: relative;
  height:140rpx;
  font-size: 28rpx;
}

.nav dt {
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  top: 20rpx;
  left: 50%;
  transform: translateX(-50%);
  background-size: 100% 100%;
}

.zcl dt {
  background-image: url(http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/icon-zcl.png);
}

.wzt dt {
  background-image: url(http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/icon-wzt.png);
}

.zdz dt {
  background-image: url(http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/icon-zdz.png);
}

.tcc dt {
  background-image: url(http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/icon-dcm.png);
}

.pth dt {
  background-image: url(http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/icon-pth.png);
}

.nav dd {
  margin-top: 85rpx;
  display: block;
}
/* /nav */
/* =====================
   /template
 ===================== */

