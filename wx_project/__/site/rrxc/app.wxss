/* fixbtn */
.fixbtn {
  position:fixed;
  bottom:30rpx;
  right:10rpx;
  width:100rpx;
  min-height:45px;
  overflow: hidden;
}

.fixbtn image {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 60rpx;
  height: 60rpx;
}

.fixbtn .my_message{
    width: 80rpx;
    height: 80rpx;
    background: #fff;
    border-radius: 40rpx;
    overflow: hidden;
    box-shadow: 0px 2.2px 1.8px #c1c1c1;
    position: relative;
}
/* /fixbtn */

/* swp_tab  */
.swp_tab {
  width:90%;
  line-height:80rpx;
  margin:18rpx 40rpx;
}

.tab_item {
  display: inline-block;
  width: 142rpx;
