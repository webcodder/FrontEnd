/* my_message_top */
.my_message_top {
    width: 100%;
    height: 352rpx;
    background: #fff;
    margin-bottom: 24rpx;
    overflow: hidden;
}

.my_photo{
    width: 100%;
    height: 252rpx;
    border-bottom: solid 1px #e6e6e6;
    position: relative;
}

.my_photo_cot {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.my_photo_cot .photo{
    width: 120rpx;
    height: 120rpx;
    margin-bottom: 10rpx;
    border-radius: 60rpx;
    overflow: hidden;
}

.my_photo_cot .photo image{
  width: 100%;
  height: 100%;
}

.my_photo_cot .nickname{
  margin-top:12rpx;
  display:block;
  font-weight: bold;
  text-align:center;
}

.my_list navigator{
    display: inline-block;
    width: 372rpx;
    height: 100rpx;
    font-size: 20rpx;
    text-align: center;
    position: relative;
}

.my_list .myinfo{
  border-right: solid 1px #e5e5e5;
}

.myinfo_pic, .follow_pic{
    width: 60rpx;
    height: 60rpx;
    margin: 10rpx auto 1rpx;
    overflow: hidden;
}

.myinfo_pic image, .follow_pic image{
  width: 100%;
  height: 100%;
}

.myinfo text, .follow text{
  margin-top: -3rpx;
  display: block;
}

.myinfo_pic{
  position: relative;
}

.myinfo_pic::after{
    position: absolute;
    content: '';
    width: 12rpx;
    height:  12rpx;
    background: #f00;
    top: 12rpx;
    right: 9rpx;
    border-radius: 10rpx;
}
/* /my_message_top */

/* my_message_cont */
.my_message_cont{
  margin-bottom: 108rpx;
}

.mymsgc{
  width: 100%;
  background: #fff;
  margin-bottom: 16rpx;
  overflow: hidden;
  min-height: 100rpx;
}

/* my_order */
.my_order_top{
  height: 100rpx;
  border-bottom: solid 1px #e6e6e6;
  line-height: 100rpx;
}

.my_order_top_cot{
    width: 650rpx;
    margin: 0 76rpx 0 24rpx;
    position: relative;
}

.my_order_top_cot text{
  font-size: 30rpx;
}

.my_arror{
  float: right;
}

.my_arror::after{
  content:'';
  width:26rpx;
  height:26rpx;
  background:url(http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/icon-wdzk.png) top no-repeat;
  right:-37rpx;
  position:absolute;
  background-size:100%;
  top:49%;
  transform:translateY(-50%);
}

.my_order_detail{
  min-height: 135rpx;
}

.modtc_cot{
  width: 653rpx;
  margin: 0 48rpx;
}

.modtc_cot navigator{
  display: inline-block;
  margin: 8rpx;
  padding:0 15rpx;
}

.modtc_cot navigator:last-child{
  margin-right: 0;
}

.modtc_pic{
  width: 80rpx;
  height: 80rpx;
  margin: 12rpx auto 10rpx;
  overflow: hidden;
}

.modtc_pic image{
  width: 100%;
  height: 100%;
}

.modtc_cot text{
  display: block;
}
/* /my_order */

/* mymsgc_wp */
.mymsgc_wp{
  border-bottom: solid 1px #efefef;
  margin-bottom: 0;
}

.mymsgc_wp navigator{
  width: 100%;
  height: 100rpx;
}

.mymsgc_ct{
  padding: 0 24rpx;
}

.mymsgc_ct text{
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
}

.mymsgc_ct .mymsgc_pic{
  float: right;
  width: 30rpx;
  height: 30rpx;
  margin-top: 36rpx;
  overflow: hidden;
}

.mymsgc_ct image{
  width: 100%;
  height: 100%;
}
