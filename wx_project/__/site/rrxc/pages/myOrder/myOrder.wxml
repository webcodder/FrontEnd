      <view class="modrct_wrap">
          <navigator url="/pages/orderState/orderState">
            <view class="modrct_title">
              <view class="modrct_cot">
                <view class="mdte_left">
                  <view class="mdte_left_pic">
                    <image src="http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/cs/my_photo.jpg"></image>
