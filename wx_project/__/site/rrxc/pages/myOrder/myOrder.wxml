<!-- swiper -->
<!-- tab -->
<view class="swp_tab">
  <!-- all -->
  <view class="all swiper-tab-list {{currentTab==0 ? 'active' : ''}}" data-current="0" bindtap="swichNav">全部</view>
  <!-- /all  -->

  <!-- dfk -->
  <view class="dfk swiper-tab-list {{currentTab==1 ? 'active' : ''}}" data-current="1" bindtap="swichNav">待付款</view>
  <!-- /dfk  -->

  <!-- jxz -->
  <view class="jxz swiper-tab-list {{currentTab==2 ? 'active' : ''}}" data-current="2" bindtap="swichNav">进行中</view>
  <!-- /jxz -->

  <!-- dsh -->
  <view class="dsh swiper-tab-list {{currentTab==3 ? 'active' : ''}}" data-current="3" bindtap="swichNav">待收货</view>
  <!-- /dsh -->

  <!-- dpj -->
  <view class="dpj swiper-tab-list {{currentTab==4 ? 'active' : ''}}" data-current="4" bindtap="swichNav">待评价</view>
  <!-- /dpj -->

  <!-- tk -->
  <view class="tk swiper-tab-list {{currentTab==5 ? 'active' : ''}}" data-current="5" bindtap="swichNav">退款</view>
  <!-- /tk -->
</view>
<!-- /tab -->

