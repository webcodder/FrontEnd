/* swp_tab */
.swp_tab{
  width: 100%;
  height: 102rpx;
  margin: 0;
  background: #fff;
  border-bottom: 2rpx solid #efefef;
  text-align: center;
  line-height: 102rpx;
  margin-bottom: 12rpx;
}

.swiper-tab-list{
  display: inline-block;  
  font-size: 28rpx;
  width: 125rpx;
  text-align: center;
}

.active{
  color: #00cd00;
  border-bottom: solid 3rpx #00cd00;
  background: initial;
}
/* /swp_tab */


