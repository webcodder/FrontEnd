.busntail_header{
    height: 186rpx;
    background: #fff;
    overflow: hidden;
    position: relative;
}

.butl_pic{
  width: 98rpx;
  height: 98rpx;
  border-radius: 49rpx;
  overflow: hidden;
  margin: 20rpx auto 0;
}

.busntail_header text{
  width:75px;
  margin:10rpx auto 0;
  position:relative;
  display:block;
}

.busntail_header text::after{
    position: absolute;
    content: '';
    width: 33rpx;
    height: 33rpx;
    background: url(http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/lxmj.png) top no-repeat;
    background-size: 100%;
    margin-right: 20rpx;
    right: -50rpx;
}

.busntail_address{
    height: 70rpx;
    line-height: 70rpx;
    overflow: hidden;
}

.busntail_address text {
    position: relative;
    margin-left: 25rpx;
}

.busntail_address text::before{
    position: absolute;
    content: '';
    width: 20rpx;
    height: 25rpx;
    background: url(http://jxc-admin.oss-cn-hangzhou.aliyuncs.com/images/icon-dw.png) top no-repeat;
    background-size: 100%;
    top: 50%;
    transform: translateY(-50%);
    left: -25rpx;
}

.busntail_nav{
    height: 88rpx;
    background: #fff;
    line-height: 83rpx;
    overflow: hidden;
    border-bottom: solid 1px #efefef;
}

.busntail_nav text {
  display: inline-block;
  width: 186rpx;
  text-align: center;
}

.busntail_nav text.active {
  color: #333;
  background: none;
  border-bottom: solid 3px #00cd00;
}
