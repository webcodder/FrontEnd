/* busntail_btn */
.busntail_btn {
    width: 100%;
    height: 98rpx;
    background: #fff;
    position: fixed;
    bottom: 0;
    overflow: hidden;
    border-top: solid 1px #efefef;
    z-index: 1;
}

.busntail_btn navigator, .daohang {
    display: block;
    float: left;
    width: 112rpx;
    font-size: 24rpx;
    height: 98rpx;
    border-right: solid 1px #efefef;
    position: relative;
}

.busbtn_pic{
    width: 50rpx;
    height: 50rpx;
    margin: 11rpx auto 0;
}

.busntail_btn navigator.sjzy text{
    color: #fff;
}

.busntail_btn navigator.sjzy {
    background: #00cd00;
    width: 150rpx;
    border-right: 0;
}

.busntail_btn navigator.vrqj .busbtn_pic image {
    transform: rotate(-180deg);
    margin-top: 0.02rem;
    margin-bottom: 0.06rem;
}

.busntail_btn navigator.vrqj {
    background: #fff;
    width: 114rpx;
}

.busntail_btn navigator.vrqj text {
    color: #00cd00;
}

.busntail_btn navigator text, .daohang text{
  display: block;
  color: #333;
  text-align: center;
  margin-top: 3rpx;
}

.busntail_btn navigator:last-child{
  border-right: none;
}
/* /busntail_btn */


/*mask*/ 
.drawer_screen { 
