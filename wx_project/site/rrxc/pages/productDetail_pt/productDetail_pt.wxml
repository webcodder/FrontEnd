<!-- product_banner -->
<view class="product_banner">
  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-color="#fff" indicator-active-color="#00cd00">  
    <block wx:for="{{imgUrls}}" wx:key="key">  
      <swiper-item>  
        <image src="{{item}}" />
      </swiper-item>  
    </block>
  </swiper>
</view>
<!-- /product_banner -->

